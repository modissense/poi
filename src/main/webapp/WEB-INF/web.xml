  <servlet-mapping>
    <servlet-name>poiShowTrendingEvents</servlet-name>
    <url-pattern>/showtrendingevents/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>poiGetPOI</servlet-name>
    <url-pattern>/getpoi/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>poiGetGPSTrace</servlet-name>
    <url-pattern>/getgpstrace/*</url-pattern>
  </servlet-mapping>
</web-app>